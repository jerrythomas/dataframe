{
	"name": "@jerrythomas/dataframe",
	"version": "1.0.0",
	"description": "Dataframe supporting join, aggregation, sorting operations.",
	"author": "Jerry Thomas <me@jerrythomas.name>",
	"license": "MIT",
	"main": "index.js",
	"svelte": "src/index.js",
	"module": "src/index.js",
	"types": "dist/index.d.ts",
	"type": "module",
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"prepublishOnly": "tsc",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. .",
		"test:ci": "vitest run",
		"test:ui": "vitest --ui",
		"test": "vitest",
		"coverage": "vitest run --coverage",
		"release": "release-it --preRelease=next",
		"upgrade": "pnpm upgrade"
	},
	"exports": {
		"./package.json": "./package.json",
		".": "./src/index.js",
		"./index.d.ts": "./dist/index.d.ts"
	},
	"devDependencies": {
		"@types/d3-array": "^3.0.4",
		"@types/ramda": "^0.28.25",
		"@vitest/ui": "~0.12.10",
		"c8": "^7.13.0",
		"eslint": "^8.39.0",
		"eslint-config-prettier": "^8.8.0",
		"js-yaml": "^4.1.0",
		"jsdom": "^20.0.3",
		"prettier": "^2.8.8",
		"typescript": "~4.6.4",
		"vite": "^4.3.2",
		"vitest": "~0.12.10"
	},
	"dependencies": {
		"@lukeed/uuid": "^2.0.1",
		"d3-array": "^3.2.3",
		"d3-collection": "^1.0.7",
		"ramda": "^0.28.0",
		"svelte": "^3.58.0"
	}
}
